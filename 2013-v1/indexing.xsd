	<?gb sect=2 en="Identifier Types" nl="Identificatie types" id="Id-types"?>
	<xs:simpleType name="personId">
		<xs:restriction base="xs:NCName">
			<xs:pattern value="p\d+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="placeId">
		<xs:restriction base="xs:NCName">
			<xs:pattern value="l\d+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="personOrPlaceId">
		<xs:restriction base="xs:NCName">
			<xs:pattern value="[lp]\d+"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="partType">
		<xs:restriction base="xs:positiveInteger">
			<xs:minInclusive value="1"/>
			<xs:maxExclusive value="10000"/>
		</xs:restriction>
	</xs:simpleType>

