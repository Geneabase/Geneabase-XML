	<?gb sect=3 en="Event Type and Subtypes" nl="Event type en subtypes" id="Event-types"?>
	<xs:complexType name="eventType">
		<xs:sequence>
			<xs:element name="person" type="i:personType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="place" type="i:placeType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="group" type="i:groupType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="literalType" type="g:string100" minOccurs="0"/>
			<xs:element name="literalDate" type="g:string100" minOccurs="0"/>
