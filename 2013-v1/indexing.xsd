	<?gb sect=3 en="Person Relation Types" nl="Persoon relatie types" id="Person-relation-types"?>
	<xs:complexType name="personToFeatureType">
		<xs:complexContent>
			<xs:extension base="i:relationType">
				<xs:attribute name="person" type="i:personIdRef"/>
				<xs:attribute name="feature" type="i:personFeatureIdRef"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="personToEventType">
		<xs:complexContent>
			<xs:extension base="i:relationType">
				<xs:attribute name="person" type="i:personIdRef"/>
				<xs:attribute name="event" type="i:eventIdRef"/>
				<xs:attribute name="personRole" type="i:personToEventEnum"/>
			</xs:extension>
			<!-- we do not allow a role to bed defined from Event to Person. this is defined by the name of the event, combined by the role of the person -->
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="personToGroupType">
		<xs:complexContent>
			<xs:extension base="i:relationType">
				<xs:attribute name="person" type="i:personIdRef"/>
				<xs:attribute name="group" type="i:groupIdRef"/>
				<xs:attribute name="personRole" type="i:personToGroupEnum"/>
				<xs:attribute name="groupRole" type="i:groupToPersonEnum"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="personToPersonType">
		<xs:complexContent>
			<xs:extension base="i:relationType">
				<xs:attribute name="person1" type="i:personIdRef"/>
				<xs:attribute name="person2" type="i:personIdRef"/>
				<xs:attribute name="role1" type="i:personToPersonEnum"/>
				<xs:attribute name="role2" type="i:personToPersonEnum"/>
