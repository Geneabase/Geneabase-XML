	<?gb sect=1 en="Types" nl="Types" id="Types"?>
	<?gb sect=2 en="String Types" nl="String types" id="String-types"?>
	<xs:annotation>
		<xs:documentation xml:lang="en">
			The following types are a generic set of string and
			number types, used by various elements.
		</xs:documentation>
		<xs:documentation xml:lang="nl">
			De volgende types zijn een generieke set van string
			en nummer types, en worden gebruikt door verschillende elementen.			
			Het is de bedoeling dat dit <strong>test</strong>balblab
		</xs:documentation>
	</xs:annotation>

	<xs:simpleType name="housenumber">
		<xs:annotation>
			<xs:documentation xml:lang="en">Housenumber on a street, specified as a single number, or '*" to indicate all numbers. </xs:documentation>
			<xs:documentation xml:lang="nl">Huisnummer van een straat, aangeduid met een enkelvoudig getal, of '*' om alle nummers aan te duiden.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\*|[0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
