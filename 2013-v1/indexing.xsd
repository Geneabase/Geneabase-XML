	<?gb sect=3 en="Other Enumeration Types" nl="Overige enumeratie types" id="Other-enumerations"?>

	<xs:simpleType name="eventTypesEnum">
		<xs:restriction base="xs:string">
			<xs:enumeration value="adoption">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		This type is for events related to childhood adoption. Typically the event is added to the individual being adopted, with this person playing the primary role. Administrators, civil servants and parents can be added to the event with different event roles.
                		
                		The adoption event can be added for the process of adoption (which can take several months), but also as a placeholder for an adoption certificate that you have as a source.
                		
                		On adding the person to the family he is adopted into, one should also set his relationship with his adoptive parents to adopted. This doubles the event information somewhat, but is used in some reports. 
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        adoptie
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="adultChristening">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The religious event of baptizing (not LDS) and/or naming an adult person 
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        doping als volwassene
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="alternateMarriage">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		An other marriage for the individual, out of active family. 
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        alternatief huwelijk
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="annulment">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		Declaring a marriage void from the beginning (never existed).
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        nietigverklaring
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="arrival">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The event of baptism (not LDS), performed in infancy or later.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        aankomst
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="baptism">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The event of baptism (not LDS), performed in infancy or later.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        doop
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="barMitzvah">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The ceremonial event held when a Jewish boy reaches age 13.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        bar mitswa
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="basMitzvah">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The ceremonial event held when a Jewish girl reaches age 13, also known as "Bat Mitzvah".
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        bas mitswa
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="birth">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The event of entering into life.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        geboorte
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="blessing">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		A religious event of bestowing divine care or intercession. Sometimes given in connection with a naming ceremony.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        zegening
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="burial">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The event of the proper disposing of the mortal remains of a deceased person.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        begrafenis
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="causeOfDeath">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		A description of the cause of death.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        (verklaring van) doodsoorzaak
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="census">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The event of the periodic count of the population for a designated locality, such as a national or state Census.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        volkstelling
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="christening">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The act of baptizing and/or naming a child.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        naamgeving van (een baby) na de doop
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="confirmation">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The religious event (not LDS) of conferring the gift of the Holy Ghost and, among protestants, full church membership.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        bevestiging
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="cremation">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		Disposal of the remains of a person's body by fire.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        crematie
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="death">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		The event when mortal life terminates.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        overlijden
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="degree">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        (behalen van) universitaire graad
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="departure">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        vertrek
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="divorce">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		An event of dissolving a marriage through civil action.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        echtscheiding
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="divorceFiling">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		An event of filing for a divorce by a spouse.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        deponering van echtscheidingspapieren
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="education">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		Indicator of a level of education attained.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        opleiding
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="election">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        verkiezing, verkozen zijn
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="emigration">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		An event of leaving one's homeland with the intent of residing elsewhere.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        emigratie
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="engagement">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		An event of recording or announcing an agreement between two people to become married.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        verloving
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="firstCommunion">
				<xs:annotation>
					<xs:documentation xml:lang="en">
                		A religious rite, the first act of sharing in the Lord's supper as part of church worship.
                	</xs:documentation>
					<xs:documentation xml:lang="nl">
                        eerste communie
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="graduation">
				<xs:annotation>
					<xs:documentation xml:lang="en">
