	</xs:complexType>
	<?gb sect=3 en="Place Relation Types" nl="Plaats relatie types" id="Place-relation-types"?>
	<xs:complexType name="placeToFeatureType">
		<xs:complexContent>
			<xs:extension base="i:relationType">
				<xs:attribute name="place" type="i:placeIdRef"/>
				<xs:attribute name="feature" type="i:personFeatureIdRef"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="placeToEventType">
		<xs:complexContent>
			<xs:extension base="i:relationType">
				<xs:attribute name="place" type="i:placeIdRef"/>
				<xs:attribute name="event" type="i:eventIdRef"/>
				<xs:attribute name="placeRole" type="i:placeToEventEnum"/>
			</xs:extension>
			<!-- we do not allow a role to be defined from place to event. it's just the place, and the role is automatically: place of [name event] -->
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="placeToPersonType">
		<xs:complexContent>
			<xs:extension base="i:relationType">
				<xs:attribute name="place" type="i:placeIdRef"/>
				<xs:attribute name="person" type="i:personIdRef"/>
				<xs:attribute name="placeRole" type="i:placeToPersonEnum"/>
				<xs:attribute name="personRole" type="i:personToPlaceEnum"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

