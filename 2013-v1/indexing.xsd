	<?gb sect=1 en="Attribute groups" nl="Attribuut groepen" id="Attribute-groups"?>
	<xs:attributeGroup name="registerAttributes">
		<xs:attribute name="id" type="g:remoteIDType" use="required">
			<xs:annotation>
				<xs:documentation>The UUID of the register that this Unit is to be applied
					to.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="version" type="xs:dateTime" use="required"/>
		<xs:attribute name="repository" type="g:repositoryIDType" use="required"/>
		<xs:attribute name="code" type="g:string24" use="required"/>
		<xs:attribute name="name" type="g:string100" use="required">
			<xs:annotation>
				<xs:documentation>The name of the register.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="part" type="g:string24" use="required"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="fileAttributes">
		<xs:attribute name="id" type="i:fileId"/>
		<xs:attribute name="uuid" type="g:uuidType">
			<xs:annotation>
				<xs:documentation>The UUID that was assigned to a FolderFile after it was uploaded
					to Geneabase, using the fileUpload tool. If the UUID is not known, fileName and repositoryID
					must be specified.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="g:fileNameType">
			<xs:annotation>
				<xs:documentation>The filename that has already been uploaded to to Geneabase, using
					the fileUpload tool. This is an alternative method of specifying a filename,
					when the UUID is not (yet) known. The filename may be either with our without
					extension, but must be without a path. The minimum that is
					required to uniquely identify the file should be specified. This may include the folderName or folderID.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="folder" type="g:uuidType">
			<xs:annotation>
				<xs:documentation>The uuid of the folder, or one of its parent folders, in which this image can be located.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ext" type="g:extensionType" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="filePosAttributes">
		<xs:attribute name="geometry" type="g:rectangleType">
			<xs:annotation>
				<xs:documentation>This defines the geometric location on the Image, or on the Video,
					by defining a geometric shape in Well Known Text Format
					(WKT).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="page" type="g:positiveShortType">
			<xs:annotation>
				<xs:documentation>Only applicable if the uploaded file is of type Document (Word or
					PDF). The Page is the Nth page within the Document.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="startTime" type="xs:time">
			<xs:annotation>
				<xs:documentation>Only applicable for Audio and Video FileEntities, indicating the
					start time in milliseconds at this media that the description is
					located.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="endTime" type="xs:time">
			<xs:annotation>
				<xs:documentation>Only if StartTime is defined, this value specifies the end time in
					milliseconds at this media that the description is located.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>

	<?gb sect=1 en="Elements" nl="Elementen" id="Elements"?>
	<?gb sect=2 en="Structural Elements" nl="Structurele elementen" id="Structural-elements"?>
	<xs:element name="register">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="unbounded" ref="i:unit"/>
			</xs:sequence>
			<xs:attributeGroup ref="i:registerAttributes"/>
		</xs:complexType>
		<xs:unique name="uniqueUnitID">
			<xs:selector xpath="./i:unit"/>
			<xs:field xpath="@id"/>
		</xs:unique>
	</xs:element>
	<xs:element name="unit">
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="i:entitiesGroup" minOccurs="0" maxOccurs="unbounded"/>
				<xs:group ref="i:entityGroupsGroup" minOccurs="0" maxOccurs="unbounded"/>
				<xs:group ref="i:relationsGroup" minOccurs="0" maxOccurs="unbounded"/>
				<xs:group ref="i:featuresGroup" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="i:file" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="i:comment" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="g:remoteIDType" use="required">
				<xs:annotation>
					<xs:documentation>This is the Id that may have been imported from external
						sources, and may also be exported. The UUID should never change, and it
						should accompany this chunk of information during its entire life, whether
						that be inside or outside this system.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="prevID" type="g:remoteIDType">
				<xs:annotation>
					<xs:documentation>ID from the external exporting system for this unit, as an
						alternative or in addition to the UUID. This value is deprecated and may not
						be supported in the future. Please use uuid instead. If provided, it will be
						used as a unique key, and will replace any other item with this same
						key.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute default="false" name="deleted" type="xs:boolean" use="optional"/>
			<xs:attribute default="true" name="public" type="xs:boolean" use="optional"> </xs:attribute>
			<xs:attribute name="publicDate" type="xs:date"/>
		</xs:complexType>
		<xs:unique name="entityID">
			<xs:selector xpath=".//*"/>
			<xs:field xpath="@id"/>
		</xs:unique>
		<xs:keyref name="person1IDRef" refer="i:entityID">
			<xs:selector xpath="*"/>
			<xs:field xpath="@person1"/>
		</xs:keyref>
		<xs:keyref name="person2IDRef" refer="i:entityID">
			<xs:selector xpath="*"/>
			<xs:field xpath="@person2"/>
		</xs:keyref>
		<xs:keyref name="personIDRef" refer="i:entityID">
			<xs:selector xpath="*"/>
			<xs:field xpath="@person"/>
		</xs:keyref>
		<xs:keyref name="fileIDRef" refer="i:entityID">
			<xs:selector xpath="*"/>
			<xs:field xpath="@file"/>
		</xs:keyref>
		<xs:keyref name="featureIDRef" refer="i:entityID">
			<xs:selector xpath="*"/>
			<xs:field xpath="@feature"/>
		</xs:keyref>
		<xs:keyref name="eventIDRef" refer="i:entityID">
			<xs:selector xpath="*"/>
			<xs:field xpath="@event"/>
		</xs:keyref>
		<xs:keyref name="placeIDRef" refer="i:entityID">
			<xs:selector xpath="*"/>
			<xs:field xpath="@place"/>
		</xs:keyref>
		<xs:keyref name="groupIDRef" refer="i:entityID">
			<xs:selector xpath="*"/>
			<xs:field xpath="@group"/>
		</xs:keyref>
	</xs:element>


	<?gb sect=2 en="Entity Elements" nl="Entiteit elementen" id="Entity-elements"?>
	<xs:element name="person" type="i:personType"/>
	<xs:element name="event" type="i:eventType"/>
	<xs:element name="place" type="i:placeType"/>
	<xs:element name="group" type="i:groupType"/>

